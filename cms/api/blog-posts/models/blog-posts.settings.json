{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "date": {
      "type": "date"
    },
    "categories": {
      "collection": "category",
      "via": "blog_posts",
      "dominant": true
    },
    "author": {
      "model": "author",
      "via": "blog_posts"
    },
    "tag": {
      "via": "blog_post",
      "model": "tags"
    },
    "slug": {
      "type": "string",
      "private": false,
      "required": false,
      "unique": false
    },
    "subtitle": {
      "type": "string",
      "private": false,
      "required": false,
      "unique": false
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
